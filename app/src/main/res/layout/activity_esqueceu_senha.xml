<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FA914D"
    tools:visibility="visible">


    <TextView
        android:id="@+id/textView"
        android:layout_width="262dp"
        android:layout_height="45dp"
        android:text="Esqueceu a senha?"
        android:textColor="#F4EDE5"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="327dp"
        android:layout_height="51dp"
        android:gravity="center"
        android:text="Insira o seu e-mail e receba um link para redefini-la"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.423"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="400dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Ao entrar na Diversa In, você concorda com nossos Termos e Política de Privacidade"
        android:textColor="#F4EDE5"
        android:textSize="15dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="707dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="333dp"
        android:layout_height="51dp"
        android:background="@drawable/curved_edittext_background"
        android:backgroundTint="#F4EDE5"
        android:gravity="center_vertical"
        android:text="   E-mail"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.592"
        app:layout_editor_absoluteX="56dp"
        app:layout_editor_absoluteY="464dp"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="55dp"
        android:layout_height="47dp"
        android:clickable="true"
        android:contentDescription="TODO"
        android:visibility="visible"
        app:backgroundTint="#F4EDE5"
        app:srcCompat="@drawable/seta_direita"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="307dp"
        tools:layout_editor_absoluteY="457dp"
        tools:srcCompat="@drawable/seta_direita"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>